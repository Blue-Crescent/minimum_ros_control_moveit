<launch>
  <!-- gazeboo -->
  <include file="$(find minimum_ros_control)/launch/trobo_world.launch" />   


  <!-- Load the URDF, SRDF and other .yaml configuration files on the param server -->
  <include file="$(find moveit_trobo)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>

  <!-- MoveIt -->
  <include file="$(find moveit_trobo)/launch/move_group.launch">
    <arg name="publish_monitored_planning_scene" value="true" />
    <arg name="allow_trajectory_execution" value="true"/>  
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="false"/>
  </include>


  <!-- Rviz and load the default config to see the state of the move_group node -->

  <include file="$(find moveit_trobo)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
    <arg name="debug" value="false"/>
  </include>

</launch>
